uantile(np.arange(0, 1.1, 0.1))\n","\n","# task 2\n","upper_bound = votes_original.quantile(0.975)\n","lower_bound = votes_original.quantile(0.025)\n","votes_filtered = votes_original[(votes_original >= lower_bound) & (votes_original <= upper_bound)]\n","\n","# task 3\n","print(f'Original: Mean = {votes_original.mean():,.0f}, Median = {votes_original.median():,.0f}')\n","print(f'Filtered: Mean = {votes_filtered.mean():,.0f}, Median = {votes_filtered.median():,.0f}')"],"metadata":{"id":"GhWeL6jHWlUR","colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"status":"ok","timestamp":1694484877384,"user_tz":-420,"elapsed":3,"user":{"displayName":"Data Chilly","userId":"00857332948930478291"}},"outputId":"bdfa1ccf-0a03-4fac-cf7c-3cd9c6722c6f"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Original: Mean = 11,775, Median = 560\n","Filtered: Mean = 4,730, Median = 560\n"]}]},{"cell_type":"code","source":[],"metadata":{"id":"Za6D0QdfsJcy"},"execution_count":null,"outputs":[]}]}